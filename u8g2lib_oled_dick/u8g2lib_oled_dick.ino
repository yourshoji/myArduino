  u8g2.sendBuffer();		// transfer internal memory to the display
  delay(100);  // one second delay, but we are only displaying a static text, so it makes no difference

  counter = (counter+1) % 12; // animation counter
  // % 12 ensures that once the counter hits 12, it resets back to its own (0)

}

