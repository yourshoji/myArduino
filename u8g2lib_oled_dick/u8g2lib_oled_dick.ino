  u8g2.begin(); // start the u8g2 library
}

void loop(void) {
  u8g2.clearBuffer();		// clear the internal memory

  u8g2.drawXBMP(0,0,128,64, epd_bitmap_allArray[counter]); // coordination x, y, x-size, y-size of the images above, image array
